{
  "name": "y2mate-api",
  "version": "1.2.0",
  "description": "Unofficial Y2mate API for Nodejs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "esm"
    ],
    "target": "es2020"
  },
  "scripts": {
    "npmb": "tsup",
    "build": "tsc",
    "format": "prettier --write ./src/*.ts",
    "format:check": "prettier ./src/*.ts",
    "test": "node ./test/searchVideo.js"
  },
  "keywords": [
    "youtube",
    "video",
    "download",
    "y2mate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aiko-chan-ai/y2mate-api"
  },
  "bugs": {
    "url": "https://github.com/aiko-chan-ai/y2mate-api/issues"
  },
  "homepage": "https://github.com/aiko-chan-ai/y2mate-api",
  "author": "aiko-chan-ai",
  "license": "ISC",
  "dependencies": {
    "format-duration": "^3.0.2",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}